{
  "name": "Http/Https Watcher",
  "manifest_version": 2,
  "version": "0.1",

  "description": "Shows percentage of https and http traffic",

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_title": "History Watcher"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
