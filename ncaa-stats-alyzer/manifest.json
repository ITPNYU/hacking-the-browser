{
  "name": "NCAA Stats Analyzer for www.sports-reference.com",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery.js", "content.js" ]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
