{
  "name": "Web Request Blocking example",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Blocks some web requests",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "cat.jpg"
  ]
}
