{
  "name": "Speak My Password",
  "description": "Speaks the value of password input fields. Icon from flaticon.com/authors/freepik",
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "128": "security.png"
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery.js", "content.js" ]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tts"
  ]
}
