{
  "manifest_version": 2,
  "name": "itp sample extension",
  "version": "0.1",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "ITP SAMPLE",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": ["jquery-2.2.0.js", "content.js"]
    }
  ],
  "permissions": [ "tabs", "contextMenus", "history" ],
  "omnibox": { "keyword" : "yeah" },
  "chrome_url_overrides" : {
    "newtab": "test.html"
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle Feature Foo"
    }
  }
}
