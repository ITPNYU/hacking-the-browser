{
  "manifest_version": 2,
  "name": "broken example C",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Make page red"
  },

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],

  "content": [
    {
      "matches": ["<no_urls>"],
      "js": ["content.js"]
    }
  ]
}
